Anis_book = input()

book = input()
counter = 0

while book != "No More Books":
    counter += 1
    if book == Anis_book:
        print(f"You checked {counter - 1} books and found it.")
        break
    book = input()

if book == "No More Books":
    print(f"The book you search is not here!")
    print(f"You checked {counter} books.")
