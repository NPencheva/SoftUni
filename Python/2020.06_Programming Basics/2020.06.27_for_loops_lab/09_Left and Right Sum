numbers = int(input())

sum_left = 0
sum_right = 0

for i in range(0, numbers):
    num = int(input())
    sum_left += num

for i in range(numbers + 1, numbers * 2 + 1):
    num = int(input())
    sum_right += num

if sum_left == sum_right:
    print(f"Yes, sum = {sum_left}")
else:
    print(f"No, diff = {abs(sum_left - sum_right)}")
